      "ignore": [
        {
          "pattern": "^\\[[\\w\\+]+\\] [\\w_]+: .*(?:join|game|party|lobby)",
          "flags": "i",
          "description": "Game advertisements and spam"
        },
        {
          "pattern": "^[\\w_]+: .*(?:www\\.|discord\\.gg|\\.com|\\.net|\\.org)",
          "flags": "i",
          "description": "URL spam"
        },
        {
          "pattern": "^[\\w_]+: .*(?:youtube|twitch|stream|video)",
          "flags": "i",
          "description": "Media spam"
        },
        {
          "pattern": "^[\\w_]+: .*(?:sub|subscribe|follow|like|click)",
          "flags": "i",
          "description": "Social media spam"
        },
        {
          "pattern": "^[\\w_]+: .*(?:free|giveaway|win|prize)",
          "flags": "i",
          "description": "Giveaway spam"
        },
        {
          "pattern": "^[\\w_]+: .*(?:hack|cheat|exploit)",
          "flags": "i",
          "description": "Cheat/hack spam"
        },
        {
          "pattern": "^([\\w_]+): .*(?:coins|gems|skyblock|sb)",
          "flags": "i",
          "description": "Hypixel specific spam"
        },
        {
          "pattern": "^([\\w_]+): .*(?:carrying|boost|service)",
          "flags": "i",
          "description": "Boosting service spam"
        },
        {
          "pattern": "^[\\w_]+: .*(?:friend request|guild invite)",
          "flags": "i",
          "description": "Friend/guild advertisements"
        },
        {
          "pattern": "^[\\w_]+: .*(?:looking for guild|lfg|recruiting)",
          "flags": "i",
          "description": "Guild recruitment spam"
        },
        {
          "pattern": "^Bot>",
          "flags": "none",
          "description": "Bot messages"
        },
        {
          "pattern": "^([\\w_]+): \\[BOT\\]",
          "flags": "none",
          "description": "Tagged bot messages"
        },
        {
          "pattern": "^[\\w_]+: .*(?:automatically|bot|script)",
