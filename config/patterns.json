        "join": [
          {
            "pattern": "^Guild > (\\w+) joined\\.$",
            "groups": ["username"],
            "description": "Hypixel guild join format"
          },
          {
            "pattern": "^G > (\\w+) joined\\.$",
            "groups": ["username"],
            "description": "Hypixel short guild join format"
          },
          {
            "pattern": "§2Guild member §r(\\w+)§r joined",
            "groups": ["username"],
            "description": "Formal join message with color codes"
          }
        ],
        "disconnect": [
          {
            "pattern": "^Guild > (\\w+) left\\.$",
            "groups": ["username"],
            "description": "Hypixel guild leave format"
          },
          {
