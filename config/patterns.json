          {
            "pattern": "^\\[([^\\]]+)\\] (\\w+) was promoted from (.+) to (.+)$",
            "groups": ["rank", "username", "fromRank", "toRank"],
            "description": "Hypixel promotion with rank prefix"
          },
          {
            "pattern": "^\\[([^\\]]+)\\] (\\w+) was promoted to (.+)$",
            "groups": ["rank", "username", "toRank"],
            "description": "Hypixel promotion simple"
          },
          {
            "pattern": "(\\w+) was promoted from (.+) to (.+)",
            "groups": ["username", "fromRank", "toRank"],
            "description": "Standard promotion message"
